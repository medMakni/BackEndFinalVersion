			targetRef="endevent1">
			<conditionExpression xsi:type="tFormalExpression">${isFinished ==
				true}
			</conditionExpression>
		</sequenceFlow>
		<sequenceFlow id="flow11" name="notfinished" sourceRef="parallelgateway3"
			targetRef="usertask4">
			<conditionExpression xsi:type="tFormalExpression">${isFinished ==
				false}
				</conditionExpression>
		</sequenceFlow>
		<endEvent id="endevent2" name="End"></endEvent>
		<sequenceFlow id="flow12" name="finished" sourceRef="parallelgateway2"
	targetRef="endevent2">
	<conditionExpression xsi:type="tFormalExpression">${isFinished ==
		true}
